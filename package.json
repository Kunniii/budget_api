{
  "name": "budget_api",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "prebuild": "bun install && bun prisma db push",
    "build": "bun build --minify --target=bun --outfile=dist/index.js src/index.ts",
    "start": ".bun/bun /dist/index.js",
    "dev": "bun --watch ./src/index.ts"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "prettier": "^3.2.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bun": "latest",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "aws-sdk": "^2.1590.0",
    "mock-aws-s3": "^4.0.2",
    "nock": "^13.5.4",
    "prisma": "^5.11.0"
  }
}
